{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "user_id"
    ],
    "properties": {
        "user_id": {
            "type": "string",
            "minLength": 1,
            "maxLength": 30
        },
        "username": {
            "type": "string",
            "minLength": 1,
            "maxLength": 30
        },
        "password": {
            "type": "string",
            "minLength": 8
        },
        "email": {
            "type": "string",
            "pattern": "^\\S+@\\S+\\.\\S+$",
            "format": "email",
            "minLength": 6,
            "maxLength": 150
        },
        "first_name": {
            "type": "string",
            "minLength": 1,
            "maxLength": 50
        },
        "last_name": {
            "type": "string",
            "minLength": 1,
            "maxLength": 50
        },
        "country": {
            "type": "string",
            "minLength": 1
        },
        "permissions": {
            "type": "integer"
        }
    },
    "examples": [{
        "user_id": 546,
        "username": "billybob",
        "password": "agEa3%fa",
        "email": "billybob@gmail.com",
        "first_name": "Billy",
        "last_name": "Bob",
        "country": "Columbia",
        "permissions": 1
    }]
}