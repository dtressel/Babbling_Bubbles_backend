{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "userId",
        "gameType",
        "soloScoreId",
        "soloStatId",
        "score",
        "numOfWords",
        "bestWords"
    ],
    "properties": {
        "userId": {
            "type": "integer",
            "default": 0,
            "title": "The userId Schema",
            "examples": [
                532
            ]
        },
        "gameType": {
            "type": "string",
            "default": "",
            "title": "The gameType Schema",
            "enum": [
                "solo3",
                "solo10",
                "free"
            ],
            "examples": [
                "solo3"
            ]
        },
        "soloScoreId": {
            "type": "integer",
            "default": 0,
            "title": "The soloScoreId Schema",
            "examples": [
                52
            ]
        },
        "soloStatId": {
            "type": "integer",
            "default": 0,
            "title": "The soloStatId Schema",
            "examples": [
                134
            ]
        },
        "score": {
            "type": "integer",
            "default": 0,
            "title": "The score Schema",
            "examples": [
                354
            ]
        },
        "numOfWords": {
            "type": "integer",
            "default": 0,
            "title": "The numOfWords Schema",
            "examples": [
                21
            ]
        },
        "bestWords": {
            "type": "array",
            "default": [],
            "title": "The bestWords Schema",
            "items": {
                "type": "object",
                "default": {},
                "title": "A Schema",
                "required": [
                    "type",
                    "word",
                    "score",
                    "boardState"
                ],
                "properties": {
                    "type": {
                        "type": "string",
                        "default": "",
                        "title": "The type Schema",
                        "enum": [
                            "bst",
                            "crz",
                            "lng"
                        ],
                        "examples": [
                            "bst"
                        ]
                    },
                    "word": {
                        "type": "string",
                        "default": "",
                        "title": "The word Schema",
                        "examples": [
                            "kiwi"
                        ]
                    },
                    "score": {
                        "type": "integer",
                        "default": 0,
                        "title": "The score Schema",
                        "examples": [
                            60
                        ]
                    },
                    "boardState": {
                        "type": "string",
                        "default": "",
                        "title": "The boardState Schema",
                        "examples": [
                            "ABcDefghijklmnopqrst122"
                        ]
                    }
                },
                "examples": [{
                    "type": "bst",
                    "word": "kiwi",
                    "score": 60,
                    "boardState": "ABcDefghijklmnopqrst122"
                }]
            },
            "examples": [
                [{
                    "type": "bst",
                    "word": "kiwi",
                    "score": 60,
                    "boardState": "ABcDefghijklmnopqrst122"
                }]
            ]
        }
    },
    "examples": [{
        "userId": 532,
        "gameType": "solo3",
        "soloScoreId": 52,
        "soloStatId": 134,
        "score": 354,
        "numOfWords": 21,
        "bestWords": [{
            "type": "bst",
            "word": "kiwi",
            "score": 60,
            "boardState": "ABcDefghijklmnopqrst122"
        }]
    }]
}