            "examples": [
                "a,b,c,d,e,f,g,h,i,j,k,l,m,n,o,p,q,r,s,t"
            ]
        }
    },
    "examples": [{
        "userId": 5342,
        "gameType": 0,
        "gameId": 4326,
        "playTime": "2004-10-19 10:23:54+02",
        "score": 145,
        "numOfWords": 23,
        "avgWordScore": 5.65,
        "bestWord": "zuchini",
