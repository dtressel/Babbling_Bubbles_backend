            "title": "The userId Schema",
            "examples": [
                5342
            ]
        },
        "gameType": {
            "type": "integer",
            "default": 0,
            "title": "The gameType Schema",
            "examples": [
                0
            ]
        },
        "gameId": {
            "type": "integer",
            "default": 0,
            "title": "The gameId Schema",
            "examples": [
                4326
            ]
        },
        "playTime": {
            "type": "string",
            "default": "",
            "title": "The playTime Schema",
            "examples": [
                "2004-10-19 10:23:54+02"
            ]
        },
        "score": {
            "type": "integer",
            "default": 0,
            "title": "The score Schema",
            "examples": [
                145
            ]
        },
        "numOfWords": {
            "type": "integer",
            "default": 0,
