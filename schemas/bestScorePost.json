{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "gameType",
        "scoreType",
        "score"
    ],
    "properties": {
        "gameType": {
            "type": "string",
            "enum": [
                "solo3",
                "solo10",
                "free"
            ]
        },
        "scoreType": {
            "type": "string",
            "enum": [
                "ttl",
                "avg"
            ]
        },
        "score": {
            "type": "integer",
            "minimum": 0
        }
    },
    "additionalProperties": false,
    "examples": [{
        "gameType": "solo3",
        "scoreType": "ttl",
        "score": 256
    }]
}